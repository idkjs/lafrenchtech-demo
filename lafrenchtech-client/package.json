{
  "name": "lafrenchtech-client",
  "version": "0.1.0",
  "description": "Relayjs, Graphql, Dockerized Apps, Multistage Docker Builds, Azure CosmosDB with MongoDB Driver, Nginx Server on Client, Deployments of Docker on Azure from Azure CLI, Create-React-App behind Nginx and a bunch of other stuff I have no business knowing",
  "keywords": [
    "relayjs",
    "graphql",
    "create-react-app",
    "Docker",
    "multi-stage",
    "azure",
    "cosmosdb",
    "mongodb",
    "deployment",
    "azure-cli",
    "styled-components",
    "nginx"
  ],
  "repository": "https://github.com/idkjs/lafrenchtech-demo.git",
  "author": {
    "name": "Alain Armand <aarmand.inbox@gmail.com",
    "url": "http://github.com/idkjs"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "react": "^16.0.0",
    "react-app-rewire-styled-components": "^3.0.0",
    "react-dom": "^16.0.0",
    "react-github-corner": "^2.0.0",
    "react-relay": "^1.4.1",
    "react-scripts": "1.0.14",
    "rebass": "^1.0.4",
    "recompose": "^0.26.0",
    "recompose-relay-modern": "^0.2.5",
    "relay-runtime": "^1.4.1",
    "reline": "^1.0.0-beta.3",
    "styled-components": "^2.2.1"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.4.1",
    "graphql-cli": "^1.1.0",
    "jest-styled-components": "^4.9.0",
    "react-app-rewired": "^1.3.3",
    "relay-compiler": "^1.4.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0"
  },
  "babel": {
    "presets": [
      "env",
      "flow"
    ]
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "schema": "graphql get-schema prod",
    "relay": "relay-compiler --src ./src --schema ./src/schema.graphql",
    "eject": "react-scripts eject",
    "lint:css": "stylelint './src/**/*.js'"
  }
}
